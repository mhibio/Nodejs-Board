<!DOCTYPE html>
<html lang="ko">
  <head>
    <%- include('../part/header.ejs') %>
    <title><%= title %></title>
  </head>
  <body>
    <%- include("../part/navbar.ejs") %>

    <main role="main" class="container">
        <div class="container">
          <div class="list-group">
            <a href="#" class="list-group-item list-group-item-action active" aria-current="true">
              Post List
            </a>

            <% for(var i = 0; i < data.length; i++) { %>
              <a href="/posting/<%= data[i].idx %>" class="list-group-item list-group-item-action list-group-item-light">
                <%= i+1 %>
                <%= data[i].title %>
              </a>
            <% } %>
          </div>
        </div>
    </main>
    
  </body>
</html>
