<!DOCTYPE html>
<html lang="ko">
  <head>
    <%- include('../part/header.ejs') %>
    <title><%= title %></title>
  </head>
  <body>
    <%- include("../part/navbar.ejs") %>
    
    <main role="main" class="container">
      <div class="container">

        <form class="form-signin" method="post" action="/post/write_post">
          <div class="form-group">
            <h1>Title</h1>
            <input type="text" class="form-control" name="title" placeholder="title">
            <br>
          </div>
          <div class="form-group">
            <h2>Content</h2>
            <textarea class="form-control" name="content" rows="17"></textarea>
          </div>

          <a class="btn btn-primary btn-lg" href="/post/list" role="button">Back</a>
          <button type="submit" class="btn btn-primary btn-lg pull-right">Submit</button>
        </form>
           
      </div>
    </main>
  </body>
</html>
